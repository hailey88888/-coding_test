public int[] solution(int[] num_list) {
        
        int a = 0;
        int b = 0;
        int[] answer = new int[2]; // 틀린부분 
        for(int i =0; i<num_list.length;i++){
            if(num_list[i]%2==0){
                a++;    
            }else{b++;}
        }
        answer[0]=a;  //놓친부분 
        answer[1]=b;  //놓친부분 
        return answer;
    }
}
